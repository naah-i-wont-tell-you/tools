{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/index", "destination": "/index.html" },
    { "source": "/merge", "destination": "/merge.html" },
    { "source": "/split", "destination": "/split.html" },
    { "source": "/watermark", "destination": "/watermark.html" },
    { "source": "/rotate", "destination": "/rotate.html" },
    { "source": "/protect", "destination": "/protect_unlock.html" },
    { "source": "/unlock", "destination": "/unlock.html" },
    { "source": "/protect_unlock", "destination": "/protect_unlock.html" },
    { "source": "/signature", "destination": "/signature.html" },
    { "source": "/sign", "destination": "/signature.html" },
    { "source": "/redact", "destination": "/redact.html" },
    { "source": "/paginate", "destination": "/paginate.html" },
    { "source": "/page-numbers", "destination": "/paginate.html" },
    { "source": "/pdf", "destination": "/pdf.html" },
    { "source": "/crop", "destination": "/pdf.html" },
    { "source": "/privacy-policy", "destination": "/Privacy-policy.html" },
    { "source": "/privacy", "destination": "/Privacy-policy.html" },
    { "source": "/terms", "destination": "/t&c.html" },
    { "source": "/terms-and-conditions", "destination": "/t&c.html" },
    { "source": "/t&c", "destination": "/t&c.html" }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=()" }
      ]
    },
    {
      "source": "/:path*.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/:path*.css",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/:path*.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    },
    {
      "source": "/:path*.(png|jpg|jpeg|gif|svg|webp|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
